        android:theme="@style/Theme.Pfa_pittonet_benjamin">

        <activity android:name=".AdminSignUp" />
        <activity android:name=".UserSignUp" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".UserList" />
        <activity android:name=".UserDetails" />
        <activity android:name=".process.LevelControlActivity" />
        <activity android:name=".process.TabletPackagingActivity" />
        <activity android:name=".process.TabletSettingActivity" />
        <activity android:name=".process.LevelSettingActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>